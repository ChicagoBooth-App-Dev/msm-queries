<h1>Director #<%= @director.id %> details</h1>

Go <a href = "/directors">back</a>

<hr>

<dl>
  <dt>
    Name
  </dt>
  <dd>
    <%= @director.name %>
  </dd>

  <dt>
    Dob
  </dt>
  <dd>
    <%= @director.dob %>
  </dd>

  <dt>
    Bio
  </dt>
  <dd>
    <%= @director.bio %>
  </dd>

  <dt>
    Image
  </dt>
  <dd>
    <img src='http://robohash.org/<%= @director.image %>' style="width:250px;">
  </dd>

  <dt>
    Created at
  </dt>
  <dd>
    <%= time_ago_in_words(@director.created_at) %> ago
  </dd>
 
  <dt>
    Updated at
  </dt>
  <dd>
    <%= time_ago_in_words(@director.updated_at) %> ago
  </dd>
</dl>



<table border='1'>

</table>