<h1>Directors</h1>

<table border='1'>

  <% @list_directors.each do |a_thing| %>
    <tr>
      <td><%= a_thing.id %></td>
      <td><img src='http://robohash.org/<%= a_thing.image %>'></td>
      <td><%= a_thing.name %></td>
      <td><%= a_thing.dob %></td>
      <td>
        <a href = "/directors/<%= a_thing.id %>">
        Show details
        </a>
      </td>
    </tr>
  <% end %>

</table>